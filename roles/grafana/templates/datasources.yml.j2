apiVersion: 1

datasources:
  - name: MEK-LAB Prometheus
    type: prometheus
    access: proxy
    url: http://{{ prometheus_server }}:9090
    isDefault: true
    editable: true

  - name: MEK-LAB Loki
    type: loki
    access: proxy
    url: http://localhost:3100
    editable: true

  - name: MEK-LAB Alertmanager
    type: alertmanager
    access: proxy
    url: http://{{ prometheus_server }}:9093
    jsonData:
      implementation: prometheus
    editable: true
